        "def build_kmp_table(pattern):\n",
        "    \"\"\"\n",
        "    Build the prefix table for the KMP algorithm.\n",
        "    \"\"\"\n",
        "    table = [0] * len(pattern)\n",
        "    i, j = 0, 1\n",
        "\n",
        "    while j < len(pattern):\n",
        "        if pattern[i] == pattern[j]:\n",
        "            i += 1\n",
        "            table[j] = i\n",
        "            j += 1\n",
        "        else:\n",
        "            if i != 0:\n",
        "                i = table[i - 1]\n",
        "            else:\n",
        "                table[j] = 0\n",
        "                j += 1\n",
        "\n",
        "    return table\n",
        "\n",
        "\n",
        "def kmp_search(text, pattern):\n",
        "    \"\"\"\n",
        "    Perform the KMP search algorithm to find pattern matches in the text.\n",
        "    \"\"\"\n",
        "    matches = []\n",
        "    i, j = 0, 0\n",
        "\n",
        "    while i < len(text):\n",
        "        if pattern[j] == text[i]:\n",
        "            i += 1\n",
        "            j += 1\n",
        "\n",
        "            if j == len(pattern):\n",
        "                matches.append(i - j)\n",
        "                j = 0\n",
        "        else:\n",
        "            if j != 0:\n",
        "                j = table[j - 1]\n",
        "            else:\n",
        "                i += 1\n",
        "\n",
        "    return matches\n",
